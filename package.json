{
  "name": "experiment-react-hook-form",
  "version": "0.1.0",
  "engines": {
    "node": "20.5.1",
    "npm": "⚠️️  【yarn】 を使ってください",
    "yarn": "1.22.19"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "check-all": "npm-run-all fix -p spell-check type-check -s test",
    "check-code": "run-p lint type-check spell-check",
    "dev": "next dev",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./{src,spec}/**/*.{ts,tsx}\" --max-warnings 0 --cache --fix",
    "fix-ts-by-prettier": "prettier \"./{src,spec}/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-prettier fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:*",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "lint-ts-by-prettier": "prettier \"./{src,spec}/**/*.{ts,tsx}\" --check --cache",
    "lint:config": "prettier \"./*.{js,json}\" \"./**/*.js\" \"!**/.next\" --check",
    "lint:editorconfig": "editorconfig-checker",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-prettier lint-ts-by-eslint",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\" --no-progress",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "@hookform/resolvers": "3.2.0",
    "date-fns": "2.30.0",
    "encoding": "0.1.13",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.45.4",
    "remeda": "1.24.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.2",
    "@storybook/nextjs": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@swc/core": "1.3.69",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "cspell": "6.31.1",
    "editorconfig-checker": "5.1.1",
    "eslint": "8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-strict-check": "0.0.16",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-strict-check": "0.0.10",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "45.0.2",
    "fixpack": "4.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.6.2",
    "minimal-types": "0.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "storybook": "7.3.2",
    "typescript": "5.0.4"
  },
  "private": true,
  "resolutions": {
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  }
}
